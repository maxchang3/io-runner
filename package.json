{
  "publisher": "maxchang",
  "name": "io-runner",
  "displayName": "IO Runner",
  "version": "1.0.6",
  "private": true,
  "description": "Simplifies single-file code execution. Redirects input/output to separate views. Built for competitive programming.",
  "author": "maxchang3 <maxcfoo@gmail.com>",
  "repository": {
    "url": "https://github.com/maxchang3/io-runner"
  },
  "license": "MIT",
  "categories": [
    "Programming Languages",
    "Testing"
  ],
  "main": "./dist/extension.js",
  "icon": "res/icon.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.78.1"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "configuration": [
      {
        "title": "IO Runner",
        "properties": {
          "io-runner.defaultEncoding": {
            "description": "Default encoding for input/output",
            "type": "string",
            "enum": [
              "utf-8",
              "gbk",
              "gb18030",
              "big5",
              "euc-jp",
              "iso-2022-jp",
              "shift_jis",
              "euc-kr",
              "ibm866",
              "iso-8859-2",
              "iso-8859-3",
              "iso-8859-4",
              "iso-8859-5",
              "iso-8859-6",
              "iso-8859-7",
              "iso-8859-8",
              "iso-8859-8-i",
              "iso-8859-10",
              "iso-8859-13",
              "iso-8859-14",
              "iso-8859-15",
              "koi8-r",
              "koi8-u",
              "windows-874",
              "windows-1250",
              "windows-1251",
              "windows-1252",
              "windows-1253",
              "windows-1254",
              "windows-1255",
              "windows-1256",
              "windows-1257",
              "windows-1258",
              "macintosh",
              "x-mac-cyrillic"
            ],
            "enumDescriptions": [
              "UTF-8",
              "Simplified Chinese (GBK/CP936)",
              "Simplified Chinese (GB18030)",
              "Traditional Chinese (BIG5/CP950)",
              "Japanese (EUC-JP)",
              "Japanese (ISO-2022-JP)",
              "Japanese (Shift JIS)",
              "Korean (EUC-KR)",
              "Russian (CP866)",
              "Central European (ISO 8859-2)",
              "Western (ISO 8859-3)",
              "Baltic (ISO 8859-4)",
              "Cyrillic (ISO 8859-5)",
              "Arabic (ISO 8859-6)",
              "Greek (ISO 8859-7)",
              "Hebrew (ISO 8859-8)",
              "Hebrew (ISO 8859-8-I)",
              "Nordic (ISO 8859-10)",
              "Estonian (ISO 8859-13)",
              "Celtic (ISO 8859-14)",
              "Western (ISO 8859-15)",
              "Cyrillic (KOI8-R)",
              "Cyrillic (KOI8-U)",
              "Thai (Windows 874)",
              "Central European (Windows 1250)",
              "Cyrillic (Windows 1251)",
              "Western (Windows 1252)",
              "Greek (Windows 1253)",
              "Turkish (Windows 1254)",
              "Hebrew (Windows 1255)",
              "Arabic (Windows 1256)",
              "Baltic (Windows 1257)",
              "Vietnamese (Windows 1258)",
              "Western (Mac OS Roman)",
              "Cyrillic (Mac OS Cyrillic encoding)"
            ],
            "default": "utf-8"
          },
          "io-runner.timeout": {
            "description": "Timeout in milliseconds",
            "type": "number",
            "default": 10000
          },
          "io-runner.launchInterpreter": {
            "description": "Mapping launch type to interpreter",
            "type": "object",
            "additionalProperties": {
              "type": "string",
              "description": "launch type to interpreter"
            },
            "default": {
              "debugpy": "python",
              "python": "python",
              "node": "node"
            }
          },
          "io-runner.launchMap": {
            "description": "Mapping file extension to launch name",
            "type": "object",
            "additionalProperties": {
              "type": [
                "string",
                "array"
              ],
              "description": "launch name or array of launch names"
            },
            "default": {
              "py": [
                "Python: Current File",
                "Python Debugger: Current File"
              ],
              "c": [
                "(lldb) Launch",
                "(gdb) Launch"
              ],
              "cpp": [
                "(lldb) Launch",
                "(gdb) Launch"
              ],
              "js": [
                "Node"
              ]
            }
          }
        }
      }
    ],
    "commands": [
      {
        "command": "io-runner.run",
        "title": "IO Runner: (Compile) And Run",
        "icon": "$(run-all)",
        "enablement": "io-runner.runable && !io-runner.running"
      },
      {
        "command": "io-runner.stop",
        "title": "IO Runner: Stop",
        "icon": "$(primitive-square)",
        "enablement": "io-runner.running"
      }
    ],
    "viewsContainers": {
      "panel": [
        {
          "id": "io-runner",
          "title": "IO Runner",
          "icon": "$(symbol-event)"
        }
      ]
    },
    "views": {
      "io-runner": [
        {
          "id": "io-runner.panel",
          "name": "IO Runner",
          "type": "webview"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "io-runner.run",
          "when": "view == io-runner.panel",
          "group": "navigation"
        },
        {
          "command": "io-runner.stop",
          "when": "view == io-runner.panel",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "build": "pnpm build:prod",
    "build:dev": "tsup",
    "build:prod": "tsup --minify",
    "dev": "pnpm build:dev --watch --sourcemap",
    "vscode:prepublish": "pnpm build:prod",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "typecheck": "tsc --noEmit",
    "release": "pnpm install && bumpp && pnpm run publish"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/vscode": "^1.78.1",
    "@types/vscode-webview": "^1.57.2",
    "@vscode/vsce": "^3.0.0",
    "bumpp": "^9.1.0",
    "tsup": "^8.0.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@c4312/vscode-variables": "^1.0.0",
    "@codemirror/commands": "^6.3.3",
    "@codemirror/state": "^6.4.0",
    "@codemirror/view": "^6.24.0",
    "@uiw/codemirror-extensions-basic-setup": "^4.21.21",
    "@vscode/webview-ui-toolkit": "^1.4.0"
  }
}
